{
    "name": "palmtree/easy-collection",
    "description": "Immutable collections which provide commonly used functionality missing from arrays",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Palmer",
            "email": "andy@andypalmer.me",
            "homepage": "https://andypalmer.me"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Palmtree\\EasyCollection\\": "src"
        },
        "files": [
            "functions.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.7",
        "palmtree/php-cs-fixer-config": "^2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Palmtree\\EasyCollection\\Test\\": "tests"
        }
    },
    "scripts": {
        "fix": "@php vendor/bin/php-cs-fixer fix",
        "psalm": "@php vendor/bin/psalm --no-cache",
        "test": "@php vendor/bin/phpunit"
    }
}
